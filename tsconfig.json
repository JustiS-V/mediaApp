{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@navigation": ["src/navigation/index"],
      "@navigation/*": ["src/navigation/*"],
      "@store": ["src/store/index"],
      "@store/*": ["src/store/*"],
      "@components": ["src/components/index"],
      "@components/*": ["src/components/*"],
      "@atoms": ["src/components/atoms/index"],
      "@atoms/*": ["src/components/atoms/*"],
      "@molecules": ["src/components/molecules/index"],
      "@molecules/*": ["src/components/molecules/*"],
      "@organisms": ["src/components/organisms/index"],
      "@organisms/*": ["src/components/organisms/*"],
      "@templates": ["src/components/templates/index"],
      "@templates/*": ["src/components/templates/*"],
      "@screens": ["src/screens/index"],
      "@screens/*": ["src/screens/*"],
      "@types": ["src/types/index"],
      "@types/*": ["src/types/*"],
      "@assets": ["src/assets/index"],
      "@assets/*": ["src/assets/*"],
      "@constants": ["src/constants/index"],
      "@constants/*": ["src/constants/*"],
      "@helper": ["src/helper/index"],
      "@helper/*": ["src/helper/*"],
      "@hooks": ["src/hooks/index"],
      "@hooks/*": ["src/hooks/*"],
      "@i18n": ["src/i18n/index"],
      "@i18n/*": ["src/i18n/*"],
      "@customTypes": ["src/types/index"],
      "@customTypes/*": ["src/types/*"]
    },
    "jsx": "react-jsx"
  }
}
